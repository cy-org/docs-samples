<!-- <snippet11> -->
<configuration>
  <system.runtime.remoting>
    <application>
      <client>
        <wellknown type="Remotable, Remotable" url="tcp://localhost:9090/Remotable.rem" />
      </client>
      <channels>
        <channel ref="tcp">
          <clientProviders>
            <formatter ref="binary"/>
            <provider type="ClientSinkProvider, ClientSink"/>
          </clientProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>
<!-- </snippet11> -->
