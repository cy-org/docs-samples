<?xml version="1.0" ?>
<configuration>
  <system.web>
    <compilation debug="true" />
    <authentication mode="Windows" />
    <customErrors mode="Off" />
    <mobileControls cookielessDataDictionaryType="System.Web.Mobile.CookielessData" />

    <!-- <Snippet2>-->
    <deviceFilters>
      <filter name="isWML11" 
              compare="PreferredRenderingType" 
              argument="wml11" />
      <filter name="supportsColor" 
              compare="IsColor" 
              argument="true" />
    </deviceFilters>
    <!-- </Snippet2>-->

  </system.web>
</configuration>
