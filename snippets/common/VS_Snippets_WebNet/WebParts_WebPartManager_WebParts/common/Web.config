<?xml version="1.0"?>
<configuration>
  <system.web>
    <webParts enableExport="true">
      <personalization defaultProvider="AspNetSqlPersonalizationProvider">
        <authorization>
          <allow users="UserAccount"
                 roles="admin"
                 verbs="enterSharedScope"/>
          
        </authorization>          
      </personalization>
    </webParts>
  </system.web>
</configuration>